{
  "name": "npm-publish-testing-1-2-3",
  "version": "0.0.1",
  "description": "Boilerplate project for libraries using rollup",
  "main": "dist/bundle.umd.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "NODE_ENV=dev jest --config jest.config.json",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --watchAll --coverage",
    "build": "NODE_ENV=prod rollup -c",
    "publish": "npm build && npm publish",
    "start": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enzoborgfrantz/rollup-library-boiler.git"
  },
  "keywords": [
    "rollup"
  ],
  "author": "enzo borg frantz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/enzoborgfrantz/rollup-library-boiler/issues"
  },
  "homepage": "https://github.com/enzoborgfrantz/rollup-library-boiler#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1"
  },
  "dependencies": {}
}
